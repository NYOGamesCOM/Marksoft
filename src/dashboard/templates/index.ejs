<script>
    document.addEventListener('DOMContentLoaded', (event) => {
        const sliderWrapper = document.querySelector('.slider-wrapper');
        const slides = Array.from(document.querySelectorAll('.slide'));
        const slideWidth = slides[0].offsetWidth + 20; // Adjust margin as needed
        const totalWidth = slideWidth * slides.length;
        // Clone slides to create infinite loop effect
        slides.forEach(slide => {
            const clone = slide.cloneNode(true);
            sliderWrapper.appendChild(clone);
        });
        // Set slider wrapper width
        sliderWrapper.style.width = `${totalWidth}px`;
        // Define animation function
        const animateSlider = () => {
            sliderWrapper.style.transition = 'transform 1s ease-in-out';
            sliderWrapper.style.transform = `translateX(-${slideWidth}px)`;
            // Reset the position to the beginning after animation ends
            setTimeout(() => {
                sliderWrapper.style.transition = 'none';
                sliderWrapper.style.transform = 'translateX(0)';
                setTimeout(() => {
                    sliderWrapper.style.transition = 'transform 1s ease-in-out';
                });
            }, 1000); // Adjust delay based on animation duration
        };
        // Call the animation function at regular intervals
        setInterval(animateSlider, 3000); // Adjust the interval time as needed
    });
</script>
